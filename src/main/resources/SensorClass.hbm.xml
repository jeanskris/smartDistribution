<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.smartcar.model.SensorClass" table="sensor_class" schema="" catalog="smartcar">
        <id name="sensorId" column="sensor_id"/>
        <property name="sensorName" column="sensor_name"/>
        <property name="basicParameters" column="basic_parameters"/>
        <property name="supplier" column="supplier"/>
    </class>
</hibernate-mapping>