<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.smartcar.model.CarInfo" table="car_info" schema="" catalog="smartcar">
        <id name="carInfoId" column="car_info_id"/>
        <property name="carId" column="car_id"/>
        <property name="gpsLongitude" column="GPS_longitude"/>
        <property name="gpsLattude" column="GPS_lattude"/>
        <property name="chargeTimes" column="charge_times"/>
        <property name="power" column="power"/>
        <property name="totalMiles" column="total_miles"/>
        <property name="speedCurrent" column="speed_current"/>
        <property name="status" column="status"/>
        <property name="updateTime" column="update_time"/>
    </class>
</hibernate-mapping>